@using CJMovieTracker.Data
@model IEnumerable<Movie>


<div class="row row-cols-4 row-cols-md-5 g-4">
    @foreach(var m in Model){
        <div class="col">
            <div class="card">
             <img src=@m.getPoster() class="card-img-top">
             <div class="card-body">
                <h5 class="card-title">@m.MovieTitle</h5>
                    <p class="card-text">@m.Description</p>
            </div>
         </div>
    </div>
    }
</div>
@{

}
